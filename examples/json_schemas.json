{
  "entity_schemas": {
    "Transaction": {
      "type": "object",
      "properties": {
        "transaction_id": "INTEGER (PK)",
        "amount": "DECIMAL(10,2)",
        "status": "VARCHAR(20)",
        "details": {
          "type": "object",
          "description": "Nested object representing the 1:M relationship with Users and Categories",
          "properties": {
            "sender": "UserObject",
            "receiver": "UserObject",
            "category": "CategoryObject"
          }
        }
      }
    },
    "User": {
      "type": "object",
      "properties": {
        "user_id": "INTEGER (PK)",
        "full_name": "VARCHAR(100)",
        "phone": "VARCHAR(15)"
      }
    }
  }
}
